    {
      "parameters": {
        "jsCode": "const orderJsonKeys = (jsonObj) => {\n  if (jsonObj === null || typeof jsonObj !== 'object') return jsonObj;\n  const ordered = {};\n  Object.keys(jsonObj).sort().forEach(key => {\n    ordered[key] = jsonObj[key];\n  });\n  return ordered;\n};\n\nconst sanitizeJsonLike = (str) => {\n  const withoutHashComments = str.replace(/^\\s*#.*$/gm, '');\n  const noTrailingCommas = withoutHashComments\n    .replace(/,\\s*}/g, '}')\n    .replace(/,\\s*\\]/g, ']');\n  return noTrailingCommas;\n};\n\nconst safeParseJson = (str) => {\n  try {\n    return JSON.parse(str);\n  } catch (e) {\n    const cleaned = sanitizeJsonLike(str);\n    return JSON.parse(cleaned);\n  }\n};\n\ntry {\n  const items = $input.all();\n\n  if (Object.keys(items[0].json).includes(\"content\")) {\n    const decoded = Buffer.from(items[0].json.content, 'base64').toString();\n    const origWorkflow = safeParseJson(decoded);\n    const n8nWorkflow = items[1].json;\n\n    const orderedOriginal = orderJsonKeys(origWorkflow);\n    const orderedActual = orderJsonKeys(n8nWorkflow);\n\n    if (JSON.stringify(orderedOriginal) === JSON.stringify(orderedActual)) {\n      items[0].json.github_status = \"same\";\n    } else {\n      items[0].json.github_status = \"different\";\n      items[0].json.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n    }\n    items[0].json.content_decoded = orderedOriginal;\n\n  } else if (Object.keys(items[0].json).includes(\"data\")) {\n    const origWorkflow = safeParseJson(items[0].json.data);\n    const n8nWorkflow = items[1].json;\n\n    const orderedOriginal = orderJsonKeys(origWorkflow);\n    const orderedActual = orderJsonKeys(n8nWorkflow);\n\n    if (JSON.stringify(orderedOriginal) === JSON.stringify(orderedActual)) {\n      items[0].json.github_status = \"same\";\n    } else {\n      items[0].json.github_status = \"different\";\n      items[0].json.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n    }\n    items[0].json.content_decoded = orderedOriginal;\n\n  } else {\n    const n8nWorkflow = items[1].json;\n    const orderedActual = orderJsonKeys(n8nWorkflow);\n\n    items[0].json.github_status = \"new\";\n    items[0].json.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n  }\n\n  return items;\n\n} catch (err) {\n  const items = $input.all();\n  items[0].json.github_status = \"parse_error\";\n  items[0].json.parse_error_message = String(err);\n  return items;\n}"
      },
